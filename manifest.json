{
    "manifest_version": 2,
  
    "name": "Quick Launcher",
    "description": "Quick launcher",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon_128.png",
      "default_popup": "index.html"
    },
    "permissions": ["activeTab"]
  }
  